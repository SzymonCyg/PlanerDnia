<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Planer.MainWindow"
        Title="Planer">
    <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
        <TextBox Name="TextBox" Width="200" Height="50" Margin="20">Podaj nazwe zadania</TextBox>
        <ComboBox Name="Kategoria" Width="200" Height="50" Margin="20" SelectedIndex="0">
            <ComboBoxItem Content="Kategoria zadania"/>
            <ComboBoxItem Content="Praca"/>
            <ComboBoxItem Content="Relaks"/>
            <ComboBoxItem Content="Zakupy"/>
        </ComboBox>
    </StackPanel>
</Window>
